---
interface Props {
  no: string;
  name: string;
}

const { no, name } = Astro.props;
---

<div>
  <div class="flex items-center justify-between w-full">
    <h2
      class="text-2xl font-bold  py-8 space-x-4 -mb-4"
    >
      <span>{no}</span>
      <span>{name}</span>
    </h2>
  </div>
</div>
